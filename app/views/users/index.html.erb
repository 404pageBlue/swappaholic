<div>
	<!-- BOOTSTRAP GRID SYSTEM -->
	<div id="marketplace_grid">
		<div class="row">
			<% @users.each do |i| %>
				<div class="col-md-4">
					<ul id="item_marketplace">
						<h4 class="label label-info" id="item_label">First Name</h4>
						<li><%= i.first_name %></li>
						<h4 class="label label-info" id="item_label">Last Name</h4>
						<li><%= i.last_name %></li>
					<!-- 	<h4 class="label label-info" id="item_label">Email</h4>
						<li><#%= i.email %></li> -->
						<h4 class="label label-info" id="item_label">Barter Rating</h4>
						<li><%= i.rating %></li>
						<h4 class="label label-info" id="item_label">Location</h4>
						<li><%= i.location %></li>
						<span><%= link_to "View Profile >>", user_path(i.id) %></span>
					</ul>
				</div>
			<% end %>
		</div>
	</div>


</div>